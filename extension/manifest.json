{
  "manifest_version": 2,

  "name": "Machine Reading",
  "version": "0.0.2",
  "description": "Machine Reading Launcher",
  "content_scripts": [
    {
      "matches":[
        "<all_urls>"
      ],
      "js": ["./assets/js/content.js"]
    }
  ],

  "background": {
    "scripts": ["./assets/js/background.js"]
  }, 

  "permissions": ["tabs"],

  "browser_action": {
    "default_icon": "./assets/img/icon.png",
    "default_popup": "./assets/sketch/popup1.html",
    "default_title": "Let me read for you"
  }
}



